<svg width="800" height="400" viewBox="0 0 800 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c" stop-opacity="0.15"/>
      <stop offset="100%" stop-color="#2962FF" stop-opacity="0.15"/>
    </linearGradient>
    <linearGradient id="blueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c"/>
      <stop offset="100%" stop-color="#2962FF"/>
    </linearGradient>
    <linearGradient id="goldAccent" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#d4af37"/>
      <stop offset="100%" stop-color="#f4e9c9"/>
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur"/>
      <feFlood flood-color="#1a3a6c" flood-opacity="0.3" result="coloredBlur"/>
      <feComposite in="SourceGraphic" in2="coloredBlur" operator="over"/>
    </filter>
    <pattern id="kolam" x="0" y="0" width="50" height="50" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="30" cy="10" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="10" cy="30" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="30" cy="30" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="20" cy="20" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="40" cy="20" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="20" cy="40" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="40" cy="40" r="2" fill="#1a3a6c" opacity="0.1"/>
    </pattern>
  </defs>
  
  <!-- Background with Pattern -->
  <rect width="800" height="400" fill="url(#bgGradient)"/>
  <rect width="800" height="400" fill="url(#kolam)"/>
  
  <!-- Decorative Border with Tamil Design -->
  <rect x="20" y="20" width="760" height="360" rx="10" stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.3" stroke-dasharray="5,5" fill="none"/>
  <path d="M40 40 L60 40 M740 40 L760 40 M40 360 L60 360 M740 360 L760 360" stroke="#d4af37" stroke-width="3"/>
  
  <!-- Main Graphic: Graph and Results Dashboard -->
  <rect x="200" y="100" width="400" height="240" rx="15" fill="white" fill-opacity="0.9" filter="drop-shadow(0 10px 20px rgba(0,0,0,0.15))"/>
  <rect x="210" y="110" width="380" height="220" rx="10" fill="rgba(26, 58, 108, 0.03)" stroke="url(#blueGradient)" stroke-width="1.5"/>
  
  <!-- Dashboard Header -->
  <rect x="210" y="110" width="380" height="40" rx="10 10 0 0" fill="url(#blueGradient)"/>
  <text x="230" y="135" font-family="Arial" font-size="14" fill="white" font-weight="bold">ELECTION RESULTS DASHBOARD</text>
  <circle cx="550" cy="130" r="10" fill="white" fill-opacity="0.2"/>
  <circle cx="520" cy="130" r="10" fill="white" fill-opacity="0.2"/>
  <path d="M545 130 L555 130 M550 125 L550 135" stroke="white" stroke-width="1.5"/>
  <path d="M515 130 L525 130" stroke="white" stroke-width="1.5"/>
  
  <!-- Bar Chart -->
  <rect x="230" y="170" width="40" height="120" rx="5" fill="rgba(255, 0, 0, 0.7)" stroke="white" stroke-width="1"/>
  <rect x="280" y="200" width="40" height="90" rx="5" fill="rgba(0, 255, 0, 0.7)" stroke="white" stroke-width="1"/>
  <rect x="330" y="150" width="40" height="140" rx="5" fill="rgba(0, 0, 255, 0.7)" stroke="white" stroke-width="1"/>
  <rect x="380" y="230" width="40" height="60" rx="5" fill="rgba(255, 255, 0, 0.7)" stroke="white" stroke-width="1"/>
  <rect x="430" y="180" width="40" height="110" rx="5" fill="rgba(128, 0, 128, 0.7)" stroke="white" stroke-width="1"/>
  <rect x="480" y="190" width="40" height="100" rx="5" fill="rgba(255, 165, 0, 0.7)" stroke="white" stroke-width="1"/>
  
  <!-- X-Axis Labels -->
  <text x="240" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">DMK</text>
  <text x="290" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">ADMK</text>
  <text x="340" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">BJP</text>
  <text x="390" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">NTK</text>
  <text x="440" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">TVK</text>
  <text x="490" y="305" font-family="Arial" font-size="10" fill="#1a3a6c" text-anchor="middle">MNM</text>
  
  <!-- Grid Lines -->
  <line x1="220" y1="290" x2="580" y2="290" stroke="#1a3a6c" stroke-width="1" stroke-opacity="0.5"/>
  <line x1="220" y1="250" x2="580" y2="250" stroke="#1a3a6c" stroke-width="1" stroke-opacity="0.2" stroke-dasharray="2,2"/>
  <line x1="220" y1="210" x2="580" y2="210" stroke="#1a3a6c" stroke-width="1" stroke-opacity="0.2" stroke-dasharray="2,2"/>
  <line x1="220" y1="170" x2="580" y2="170" stroke="#1a3a6c" stroke-width="1" stroke-opacity="0.2" stroke-dasharray="2,2"/>
  <line x1="220" y1="130" x2="580" y2="130" stroke="#1a3a6c" stroke-width="1" stroke-opacity="0.2" stroke-dasharray="2,2"/>
  
  <!-- Gold Accent Line for Results -->
  <path d="M240 290 L240 170 L290 200 L340 150 L390 230 L440 180 L490 190 L490 290 Z" fill="url(#goldAccent)" fill-opacity="0.1" stroke="url(#goldAccent)" stroke-width="2"/>
  
  <!-- Clock Icon for Fast Results -->
  <circle cx="120" cy="160" r="50" fill="white" stroke="url(#blueGradient)" stroke-width="3"/>
  <circle cx="120" cy="160" r="45" fill="none" stroke="url(#goldAccent)" stroke-width="1"/>
  <circle cx="120" cy="160" r="3" fill="url(#blueGradient)"/>
  <line x1="120" y1="160" x2="120" y2="130" stroke="url(#blueGradient)" stroke-width="3" stroke-linecap="round"/>
  <line x1="120" y1="160" x2="140" y2="160" stroke="url(#blueGradient)" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Clock Markers -->
  <line x1="120" y1="115" x2="120" y2="120" stroke="#1a3a6c" stroke-width="2"/>
  <line x1="120" y1="200" x2="120" y2="205" stroke="#1a3a6c" stroke-width="2"/>
  <line x1="75" y1="160" x2="80" y2="160" stroke="#1a3a6c" stroke-width="2"/>
  <line x1="160" y1="160" x2="165" y2="160" stroke="#1a3a6c" stroke-width="2"/>
  
  <!-- Speed Lines -->
  <path d="M180 160 C200 160 200 140 220 140" stroke="url(#blueGradient)" stroke-width="3" stroke-linecap="round"/>
  <path d="M180 160 C200 160 200 180 220 180" stroke="url(#blueGradient)" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Countdown Timer -->
  <rect x="90" y="230" width="60" height="30" rx="5" fill="url(#blueGradient)"/>
  <text x="100" y="250" font-family="'Courier New', monospace" font-size="16" fill="white" font-weight="bold">00:00</text>
  
  <!-- Connected Devices (Phones/Computers) -->
  <rect x="90" y="80" width="20" height="30" rx="3" fill="white" stroke="url(#blueGradient)" stroke-width="1"/>
  <rect x="92" y="85" width="16" height="20" rx="1" fill="rgba(26, 58, 108, 0.1)"/>
  <rect x="120" y="70" width="30" height="40" rx="3" fill="white" stroke="url(#blueGradient)" stroke-width="1"/>
  <rect x="125" y="75" width="20" height="30" rx="1" fill="rgba(26, 58, 108, 0.1)"/>
  <rect x="160" y="85" width="15" height="25" rx="3" fill="white" stroke="url(#blueGradient)" stroke-width="1"/>
  <rect x="162" y="88" width="11" height="18" rx="1" fill="rgba(26, 58, 108, 0.1)"/>
  
  <!-- Connection Lines -->
  <path d="M100 110 L120 160 M135 110 L120 160 M167 110 L120 160" stroke="#1a3a6c" stroke-width="1" stroke-dasharray="3,3"/>
  
  <!-- Digital Data Transfer -->
  <text x="105" y="130" font-family="monospace" font-size="5" fill="#1a3a6c">01001</text>
  <text x="145" y="125" font-family="monospace" font-size="5" fill="#1a3a6c">10110</text>
  <text x="155" y="140" font-family="monospace" font-size="5" fill="#1a3a6c">01010</text>
  
  <!-- Live Update Indicators -->
  <g filter="url(#glow)">
    <circle cx="640" cy="130" r="6" fill="#00ff00"/>
    <text x="650" y="135" font-family="Arial" font-size="12" fill="#1a3a6c">LIVE UPDATES</text>
  </g>
  <g filter="url(#glow)">
    <circle cx="640" cy="170" r="6" fill="#00ff00"/>
    <text x="650" y="175" font-family="Arial" font-size="12" fill="#1a3a6c">REAL-TIME DATA</text>
  </g>
  <g filter="url(#glow)">
    <circle cx="640" cy="210" r="6" fill="#00ff00"/>
    <text x="650" y="215" font-family="Arial" font-size="12" fill="#1a3a6c">INSTANT COUNTING</text>
  </g>
  <g filter="url(#glow)">
    <circle cx="640" cy="250" r="6" fill="#00ff00"/>
    <text x="650" y="255" font-family="Arial" font-size="12" fill="#1a3a6c">VERIFIED RESULTS</text>
  </g>
  
  <!-- Label with Premium Button Style -->
  <rect x="300" y="350" width="200" height="36" rx="18" fill="url(#blueGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <rect x="302" y="352" width="196" height="32" rx="16" fill="url(#blueGradient)" stroke="url(#goldAccent)" stroke-width="1"/>
  <text x="340" y="375" font-family="Arial" font-size="16" fill="white" font-weight="bold">FAST RESULTS</text>
  
  <!-- Tamil Ornamental Elements -->
  <path d="M280 350 C280 350 270 340 280 330 C290 320 300 330 300 330" stroke="url(#goldAccent)" stroke-width="2"/>
  <path d="M520 350 C520 350 530 340 520 330 C510 320 500 330 500 330" stroke="url(#goldAccent)" stroke-width="2"/>
</svg> 