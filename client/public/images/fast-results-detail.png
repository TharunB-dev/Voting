<svg width="800" height="400" viewBox="0 0 800 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#2962FF" stop-opacity="0.1"/>
    </linearGradient>
    <linearGradient id="clockGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c"/>
      <stop offset="100%" stop-color="#2962FF"/>
    </linearGradient>
    <linearGradient id="barGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" stop-color="#2962FF"/>
      <stop offset="100%" stop-color="#1a3a6c"/>
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur"/>
      <feFlood flood-color="#1a3a6c" flood-opacity="0.3" result="coloredBlur"/>
      <feComposite in="SourceGraphic" in2="coloredBlur" operator="over"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bgGradient)"/>
  
  <!-- Connection Lines -->
  <path d="M100 200 L200 200 L200 100 L400 100 L400 300 L600 300 L600 200 L700 200" 
        stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.2" stroke-dasharray="5,5"/>
  <path d="M100 250 L300 250 L300 150 L500 150 L500 250 L700 250" 
        stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.2" stroke-dasharray="5,5"/>
  
  <!-- Central Circle with Clock -->
  <circle cx="400" cy="200" r="120" fill="white" filter="drop-shadow(0 10px 20px rgba(0,0,0,0.15))"/>
  
  <!-- Stylized Clock -->
  <circle cx="400" cy="200" r="80" stroke="url(#clockGradient)" stroke-width="8" fill="white" filter="url(#glow)"/>
  <circle cx="400" cy="200" r="5" fill="url(#clockGradient)"/>
  
  <!-- Clock Markings -->
  <line x1="400" y1="130" x2="400" y2="140" stroke="#1a3a6c" stroke-width="3"/>
  <line x1="400" y1="260" x2="400" y2="270" stroke="#1a3a6c" stroke-width="3"/>
  <line x1="330" y1="200" x2="340" y2="200" stroke="#1a3a6c" stroke-width="3"/>
  <line x1="460" y1="200" x2="470" y2="200" stroke="#1a3a6c" stroke-width="3"/>
  
  <!-- Clock Hands -->
  <line x1="400" y1="200" x2="400" y2="150" stroke="url(#clockGradient)" stroke-width="5" stroke-linecap="round"/>
  <line x1="400" y1="200" x2="440" y2="225" stroke="url(#clockGradient)" stroke-width="5" stroke-linecap="round"/>
  
  <!-- Lightning Bolt -->
  <path d="M650 150 L630 200 L650 200 L630 250" stroke="url(#clockGradient)" stroke-width="5" stroke-linecap="round" stroke-linejoin="round" filter="url(#glow)"/>
  
  <!-- Bar Graph - Left Side -->
  <rect x="150" y="200" width="20" height="80" rx="3" fill="url(#barGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <rect x="180" y="220" width="20" height="60" rx="3" fill="url(#barGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <rect x="210" y="180" width="20" height="100" rx="3" fill="url(#barGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <rect x="240" y="150" width="20" height="130" rx="3" fill="url(#barGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  
  <!-- Percentage Indicators -->
  <text x="150" y="190" font-family="Arial" font-size="12" fill="#1a3a6c" font-weight="bold">60%</text>
  <text x="180" y="210" font-family="Arial" font-size="12" fill="#1a3a6c" font-weight="bold">45%</text>
  <text x="210" y="170" font-family="Arial" font-size="12" fill="#1a3a6c" font-weight="bold">75%</text>
  <text x="240" y="140" font-family="Arial" font-size="12" fill="#1a3a6c" font-weight="bold">95%</text>
  
  <!-- Real-Time Data Flows -->
  <circle cx="560" cy="150" r="8" fill="#2962FF" opacity="0.8">
    <animate attributeName="opacity" values="0.8;0.2;0.8" dur="2s" repeatCount="indefinite" />
  </circle>
  <circle cx="580" cy="180" r="8" fill="#2962FF" opacity="0.5">
    <animate attributeName="opacity" values="0.5;0.1;0.5" dur="1.5s" repeatCount="indefinite" />
  </circle>
  <circle cx="560" cy="210" r="8" fill="#2962FF" opacity="0.3">
    <animate attributeName="opacity" values="0.3;0.8;0.3" dur="2.5s" repeatCount="indefinite" />
  </circle>
  
  <!-- Label -->
  <rect x="330" y="320" width="140" height="30" rx="15" fill="#1a3a6c" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <text x="345" y="340" font-family="Arial" font-size="14" fill="white" font-weight="bold">FAST RESULTS</text>
</svg> 