<svg width="800" height="400" viewBox="0 0 800 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c" stop-opacity="0.15"/>
      <stop offset="100%" stop-color="#2962FF" stop-opacity="0.15"/>
    </linearGradient>
    <linearGradient id="blueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c"/>
      <stop offset="100%" stop-color="#2962FF"/>
    </linearGradient>
    <linearGradient id="goldAccent" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#d4af37"/>
      <stop offset="100%" stop-color="#f4e9c9"/>
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur"/>
      <feFlood flood-color="#1a3a6c" flood-opacity="0.3" result="coloredBlur"/>
      <feComposite in="SourceGraphic" in2="coloredBlur" operator="over"/>
    </filter>
    <pattern id="kolam" x="0" y="0" width="50" height="50" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="30" cy="10" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="10" cy="30" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="30" cy="30" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="20" cy="20" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="40" cy="20" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="20" cy="40" r="2" fill="#1a3a6c" opacity="0.1"/>
      <circle cx="40" cy="40" r="2" fill="#1a3a6c" opacity="0.1"/>
    </pattern>
  </defs>
  
  <!-- Background with Pattern -->
  <rect width="800" height="400" fill="url(#bgGradient)"/>
  <rect width="800" height="400" fill="url(#kolam)"/>
  
  <!-- Decorative Border with Tamil Design -->
  <rect x="20" y="20" width="760" height="360" rx="10" stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.3" stroke-dasharray="5,5" fill="none"/>
  <path d="M40 40 L60 40 M740 40 L760 40 M40 360 L60 360 M740 360 L760 360" stroke="#d4af37" stroke-width="3"/>
  
  <!-- Main Graphic: Universal Access Symbol -->
  <circle cx="400" cy="190" r="120" fill="white" fill-opacity="0.9" filter="drop-shadow(0 10px 20px rgba(0,0,0,0.15))"/>
  <circle cx="400" cy="190" r="115" fill="rgba(26, 58, 108, 0.03)" stroke="url(#blueGradient)" stroke-width="1.5"/>
  
  <!-- Universal Accessibility Symbol -->
  <circle cx="400" cy="130" r="25" fill="url(#blueGradient)"/>
  <path d="M380 160 L420 160 M380 160 L370 220 M420 160 L430 220 M390 220 L410 220" stroke="url(#blueGradient)" stroke-width="10" stroke-linecap="round" stroke-linejoin="round"/>
  <path d="M400 250 C450 250 470 220 470 190 M400 250 C350 250 330 220 330 190" stroke="url(#blueGradient)" stroke-width="8" stroke-linecap="round"/>
  
  <!-- Gold Accents -->
  <circle cx="400" cy="130" r="25" fill="none" stroke="url(#goldAccent)" stroke-width="2"/>
  <path d="M380 160 L420 160 M380 160 L370 220 M420 160 L430 220 M390 220 L410 220" stroke="url(#goldAccent)" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
  <path d="M400 250 C450 250 470 220 470 190 M400 250 C350 250 330 220 330 190" stroke="url(#goldAccent)" stroke-width="2" stroke-linecap="round"/>
  
  <!-- Devices For Accessibility -->
  <!-- Computer -->
  <rect x="160" y="150" width="80" height="60" rx="5" fill="white" stroke="url(#blueGradient)" stroke-width="2"/>
  <rect x="170" y="160" width="60" height="40" rx="2" fill="rgba(26, 58, 108, 0.1)"/>
  <rect x="150" y="210" width="100" height="10" rx="5" fill="white" stroke="url(#blueGradient)" stroke-width="2"/>
  <rect x="195" y="210" width="10" height="20" fill="white" stroke="url(#blueGradient)" stroke-width="1"/>
  <rect x="180" y="230" width="40" height="5" rx="2" fill="white" stroke="url(#blueGradient)" stroke-width="1"/>
  
  <!-- Mobile Device -->
  <rect x="560" y="150" width="40" height="80" rx="8" fill="white" stroke="url(#blueGradient)" stroke-width="2"/>
  <rect x="565" y="160" width="30" height="50" rx="2" fill="rgba(26, 58, 108, 0.1)"/>
  <circle cx="580" cy="220" r="5" fill="rgba(26, 58, 108, 0.2)" stroke="url(#blueGradient)" stroke-width="1"/>
  
  <!-- Tablet -->
  <rect x="640" y="160" width="60" height="80" rx="8" fill="white" stroke="url(#blueGradient)" stroke-width="2"/>
  <rect x="645" y="170" width="50" height="60" rx="2" fill="rgba(26, 58, 108, 0.1)"/>
  <circle cx="670" cy="235" r="5" fill="rgba(26, 58, 108, 0.2)" stroke="url(#blueGradient)" stroke-width="1"/>
  
  <!-- Connection Lines -->
  <path d="M230 180 C280 180 300 150 360 150" stroke="#1a3a6c" stroke-width="2" stroke-dasharray="5,5"/>
  <path d="M560 180 C510 180 490 150 440 150" stroke="#1a3a6c" stroke-width="2" stroke-dasharray="5,5"/>
  <path d="M670 200 C600 200 580 150 440 150" stroke="#1a3a6c" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Signal Waves - Internet Access -->
  <path d="M70 100 C100 70 150 70 180 100" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <path d="M80 110 C100 90 130 90 150 110" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <path d="M90 120 C100 110 110 110 120 120" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <circle cx="105" cy="135" r="8" fill="url(#blueGradient)"/>
  
  <!-- Signal Waves - Right Side -->
  <path d="M730 100 C700 70 650 70 620 100" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <path d="M720 110 C700 90 670 90 650 110" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <path d="M710 120 C700 110 690 110 680 120" stroke="url(#blueGradient)" stroke-width="2" fill="none"/>
  <circle cx="695" cy="135" r="8" fill="url(#blueGradient)"/>
  
  <!-- Inclusive Access Icons -->
  <!-- Hearing Aid Symbol -->
  <circle cx="190" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <path d="M180 290 C180 280 190 275 200 280 C210 285 205 300 195 300" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  <path d="M190 300 L185 315" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  
  <!-- Visual Aid Symbol -->
  <circle cx="250" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <path d="M235 300 C235 300 245 285 265 300 C245 315 235 300 235 300 Z" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  <circle cx="250" cy="300" r="5" fill="#1a3a6c"/>
  
  <!-- Motor Aid Symbol -->
  <circle cx="310" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <path d="M300 310 L310 290 L320 310 M305 300 L315 300" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  
  <!-- Language Aid Symbol -->
  <circle cx="490" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <text x="480" y="306" font-family="Arial" font-size="16" fill="#1a3a6c" font-weight="bold">à®…</text>
  
  <!-- Cognitive Aid Symbol -->
  <circle cx="550" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <path d="M540 295 L550 290 L560 295 M550 290 L550 310 M545 310 L555 310" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  
  <!-- Elderly Aid Symbol -->
  <circle cx="610" cy="300" r="20" fill="white" stroke="url(#blueGradient)" stroke-width="1.5"/>
  <path d="M600 300 C600 290 610 285 620 295" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  <path d="M605 305 L610 310 L615 305" stroke="#1a3a6c" stroke-width="2" fill="none"/>
  
  <!-- Label with Premium Button Style -->
  <rect x="300" y="350" width="200" height="36" rx="18" fill="url(#blueGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <rect x="302" y="352" width="196" height="32" rx="16" fill="url(#blueGradient)" stroke="url(#goldAccent)" stroke-width="1"/>
  <text x="335" y="375" font-family="Arial" font-size="16" fill="white" font-weight="bold">ACCESSIBLE VOTING</text>
  
  <!-- Tamil Ornamental Elements -->
  <path d="M280 350 C280 350 270 340 280 330 C290 320 300 330 300 330" stroke="url(#goldAccent)" stroke-width="2"/>
  <path d="M520 350 C520 350 530 340 520 330 C510 320 500 330 500 330" stroke="url(#goldAccent)" stroke-width="2"/>
</svg> 