<svg width="800" height="400" viewBox="0 0 800 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#2962FF" stop-opacity="0.1"/>
    </linearGradient>
    <linearGradient id="lockGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c"/>
      <stop offset="100%" stop-color="#2962FF"/>
    </linearGradient>
    <linearGradient id="shieldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a6c"/>
      <stop offset="100%" stop-color="#2962FF"/>
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur"/>
      <feFlood flood-color="#1a3a6c" flood-opacity="0.3" result="coloredBlur"/>
      <feComposite in="SourceGraphic" in2="coloredBlur" operator="over"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bgGradient)"/>
  
  <!-- Connection Lines (Circuit Board Pattern) -->
  <path d="M100 200 L200 200 L200 100 L400 100 L400 300 L600 300 L600 150 L700 150" 
        stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.2" stroke-dasharray="5,5"/>
  <path d="M100 250 L300 250 L300 150 L500 150 L500 200 L700 200" 
        stroke="#1a3a6c" stroke-width="2" stroke-opacity="0.2" stroke-dasharray="5,5"/>
  
  <!-- Lock in Circle -->
  <circle cx="400" cy="200" r="120" fill="white" filter="drop-shadow(0 10px 20px rgba(0,0,0,0.15))"/>
  <path d="M400 140 C378 140 360 158 360 180 L360 200 L350 200 C344.5 200 340 204.5 340 210 L340 260 C340 265.5 344.5 270 350 270 L450 270 C455.5 270 460 265.5 460 260 L460 210 C460 204.5 455.5 200 450 200 L440 200 L440 180 C440 158 422 140 400 140 Z M400 160 C411 160 420 169 420 180 L420 200 L380 200 L380 180 C380 169 389 160 400 160 Z" 
        fill="url(#lockGradient)" filter="url(#glow)"/>
  
  <!-- Shield Top Left -->
  <path d="M150 250 L180 250 L180 280 L165 300 L150 280 Z" fill="url(#shieldGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <path d="M157.5 260 L172.5 260 L172.5 277.5 L165 287.5 L157.5 277.5 Z" fill="white" opacity="0.3"/>
  
  <!-- Shield Bottom Right -->
  <path d="M650 150 L680 150 L680 180 L665 200 L650 180 Z" fill="url(#shieldGradient)" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <path d="M657.5 160 L672.5 160 L672.5 177.5 L665 187.5 L657.5 177.5 Z" fill="white" opacity="0.3"/>
  
  <!-- Binary Data Top Right -->
  <text x="600" y="120" font-family="monospace" font-size="14" fill="#1a3a6c">10110010</text>
  <text x="600" y="140" font-family="monospace" font-size="14" fill="#1a3a6c">01001101</text>
  
  <!-- Binary Data Bottom Left -->
  <text x="200" y="280" font-family="monospace" font-size="14" fill="#1a3a6c">11010010</text>
  <text x="200" y="300" font-family="monospace" font-size="14" fill="#1a3a6c">00101101</text>
  
  <!-- Encryption Symbol Top -->
  <circle cx="400" cy="50" r="20" fill="white" stroke="#1a3a6c" stroke-width="1.5"/>
  <text x="393" y="55" font-family="Arial" font-size="16" fill="#1a3a6c" font-weight="bold">E</text>
  
  <!-- Encryption Symbol Bottom -->
  <circle cx="400" cy="350" r="20" fill="white" stroke="#1a3a6c" stroke-width="1.5"/>
  <text x="393" y="355" font-family="Arial" font-size="16" fill="#1a3a6c" font-weight="bold">E</text>
  
  <!-- Label -->
  <rect x="320" y="320" width="160" height="30" rx="15" fill="#1a3a6c" filter="drop-shadow(0 3px 6px rgba(0,0,0,0.2))"/>
  <text x="345" y="340" font-family="Arial" font-size="14" fill="white" font-weight="bold">SECURE VOTING</text>
</svg> 